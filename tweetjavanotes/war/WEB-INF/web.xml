<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <servlet>
  <servlet-name>MyNotesOnEffective_Java</servlet-name>
  <servlet-class>com.sudhakar.notesoneffectivejava.MyNotesOnEffective_JavaServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>NotesSetup</servlet-name>
  <servlet-class>com.sudhakar.notesoneffectivejava.NoteSetup</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>RssServlet</servlet-name>
  <servlet-class>com.sudhakar.notesoneffectivejava.RssServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>SendOutTwitterUpdates</servlet-name>
  <servlet-class>com.sudhakar.notesoneffectivejava.TwitterPushServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>Updatesubscribers</servlet-name>
  <servlet-class>com.sudhakar.notesoneffectivejava.UpdateTwitterSubscriberServlet</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>editnote</servlet-name>
  <servlet-class>com.sudhakar.notesoneffectivejava.EditNoteServlet</servlet-class>
 </servlet>
 <servlet> 
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value/>
  </init-param>
 </servlet>
  <servlet>
    <servlet-name>remoteapi</servlet-name>
    <servlet-class>com.google.apphosting.utils.remoteapi.RemoteApiServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>remoteapi</servlet-name>
    <url-pattern>/remote_api</url-pattern>
  </servlet-mapping> 
 <servlet-mapping>
  <servlet-name>MyNotesOnEffective_Java</servlet-name>
  <url-pattern>/notes</url-pattern>
 </servlet-mapping>
  <servlet-mapping>
  <servlet-name>editnote</servlet-name>
  <url-pattern>/edit</url-pattern>
 </servlet-mapping>
 
 <servlet-mapping>
  <servlet-name>NotesSetup</servlet-name>
  <url-pattern>/setup</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>RssServlet</servlet-name>
  <url-pattern>/rss</url-pattern>
 </servlet-mapping>
  <servlet-mapping>
  <servlet-name>SendOutTwitterUpdates</servlet-name>
  <url-pattern>/sendtwitters</url-pattern>
 </servlet-mapping>
  <servlet-mapping>
  <servlet-name>Updatesubscribers</servlet-name>
  <url-pattern>/updatesubscribers</url-pattern>
 </servlet-mapping> 
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>index.html</welcome-file>
 </welcome-file-list>
</web-app>